{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "usage",
                "corejs": {
                    "version": 3
                },
                "targets": {
                    "chrome": "60",
                    "firefox": "50",
                    "ie": "9",
                    "safari": "10",
                    "edge": "17"
                }
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "corejs": false,
                "helpers": true,
                "regenerator": true,
                "useESModules": false,
                "absoluteRuntime": "@babel/runtime"
            }
        ]
    ]
}